<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aplicativo.milreuelima.iesb.com.br.futebas" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="aplicativo.milreuelima.iesb.com.br.futebas.Atividades.MainFutebasActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.facebook.FacebookActivity"
            android:label="FutebasApp"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"
            android:screenOrientation="portrait"
            />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="1647954948749801" />

        <activity
            android:name="aplicativo.milreuelima.iesb.com.br.futebas.Atividades.ListaJogadores"
            android:label="@string/title_activity_lista_jogadores"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="aplicativo.milreuelima.iesb.com.br.futebas.Atividades.Pagamento"
            android:label="@string/title_activity_pagamento"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="aplicativo.milreuelima.iesb.com.br.futebas.Atividades.Preferencias"
            android:label="@string/title_activity_preferencias"
            android:screenOrientation="portrait"
            >
        </activity>
    </application>

</manifest>
